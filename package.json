{
  "name": "help-with-prison-visits-internal",
  "version": "1.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ministryofjustice/help-with-prison-visits-internal.git"
  },
  "engines": {
    "node": "12.x.x"
  },
  "scripts": {
    "start": "npm run css-build && node ./app/bin/www",
    "security_audit": "npx audit-ci --config audit-ci.json",
    "record-build-info": "node scripts/record-build-info",
    "test": "standard && mocha --recursive test/unit/ --exit",
    "test-coverage": "nyc npm run test-unit",
    "test-unit": "mocha --recursive test/unit/ --exit",
    "test-integration": "KNEX_CONFIG=testing mocha --recursive test/integration/ --timeout 15000 --exit",
    "test-e2e": "gulp --gulpfile gulpfile-e2e.js",
    "test-e2e-smoke": "gulp --gulpfile gulpfile-e2e.js e2e-smoke",
    "test-e2e-ie8": "wdio test/wdio.conf.sauce.ie8.js",
    "test-e2e-firefox": "wdio test/wdio.conf.sauce.firefox.js",
    "test-e2e-ie8-jenkins": "wdio test/wdio.conf.sauce.ie8.jenkins.js",
    "test-e2e-firefox-jenkins": "wdio test/wdio.conf.sauce.firefox.jenkins.js",
    "test-e2e-chrome-jenkins": "wdio test/wdio.conf.sauce.chrome.jenkins.js",
    "test-e2e-chrome-jenkins-smoke": "wdio test/wdio.conf.sauce.chrome.jenkins.smoke.js",
    "css-build": "./build-css"
  },
  "standard": {
    "ignore": [
      "jquery*.js",
      "ga.js"
    ],
    "globals": [
      "describe",
      "before",
      "beforeEach",
      "after",
      "afterEach",
      "it",
      "browser",
      "$"
    ]
  },
  "dependencies": {
    "applicationinsights": "^2.1.8",
    "aws-sdk": "^2.1022.0",
    "bluebird": "^3.7.2",
    "bunyan": "^1.8.15",
    "bunyan-prettystream": "^0.1.3",
    "compression": "^1.7.4",
    "connect-redis": "^6.0.0",
    "cookie-parser": "^1.4.5",
    "csurf": "^1.11.0",
    "csv": "^5.5.3",
    "debug": ">=4.3.2",
    "dotenv": "^10.0.0",
    "express": "~4.17.1",
    "express-sanitizer": "^1.0.6",
    "express-session": "^1.17.2",
    "govuk_frontend_toolkit": "^9.0.1",
    "govuk_template_jinja": "^0.26.0",
    "govuk-elements-sass": "^2.2.1",
    "helmet": "^4.6.0",
    "knex": "^0.95.13",
    "mkdirp": "^1.0.4",
    "moment": "^2.29.1",
    "mssql": "^7.2.1",
    "multer": "^1.4.3",
    "nunjucks": "^3.2.3",
    "on-finished": "^2.3.0",
    "passport": "^0.5.0",
    "passport-oauth2": "^1.6.1",
    "path": "^0.12.7",
    "ramda": "^0.27.1",
    "redis": "^3.1.2",
    "request": "^2.88.2",
    "sass": "^1.43.4",
    "serve-favicon": "~2.5.0",
    "underscore": "^1.13.1",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "assert": "^2.0.0",
    "chai": "^4.3.4",
    "mocha": "^9.1.3",
    "nyc": "^15.1.0",
    "proxyquire": "^2.1.3",
    "sinon": "^12.0.1",
    "standard": "^16.0.4",
    "supertest": "^6.1.6"
  }
}
